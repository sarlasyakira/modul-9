#include <iostream>

using namespace std;

int hasil{
  int hargasetelahpajak,hargamawar,hargadasar,hargafinal{
  hargasetelahpajak=hargadasar+(hargadasar*10/100);
return hasil;
  }
}

int main(){
  int hargamawar = 1000;
  int hargafinal = hargasetelahpajak;
  cout<<"Harga mawar 1 tangkai Rp. "<<hargafinal<<endl;

  return 0;
  
}

#include <iostream>
using namespace std;

int tambah(int m,int n){
  m+=5;
  n+=7;
  cout<<"\n\nNilai di dalam fungsi tambah()";
  cout<<"\nm= "<<m<<"n= "<<n;
  cout<<endl;
  return 0;
}

int main(){
  int a,b;
  a=5;
  b=9;

  cout<<"Nilai sebelum fungsi digunakan ";
  cout<<"\na= "<<a<<"b= "<<b;
  tambah(a,b);
  cout<<"\nNilai setelah fungsi digunakan ";
  cout<<"\na= "<<a<<"b= "<<b;
  getchar ();
}
